{
  "storage": {
    "available": {
      "service": "DynamoDB",
      "providerPlugin": "awscloudformation"
    },
    "sheets": {
      "service": "DynamoDB",
      "providerPlugin": "awscloudformation"
    }
  },
  "function": {
    "getAvailable": {
      "build": true,
      "providerPlugin": "awscloudformation",
      "service": "Lambda",
      "dependsOn": [
        {
          "category": "storage",
          "resourceName": "available",
          "attributes": [
            "Name",
            "Arn",
            "StreamArn"
          ]
        }
      ]
    },
    "syncAvailable": {
      "build": true,
      "providerPlugin": "awscloudformation",
      "service": "Lambda",
      "dependsOn": [
        {
          "category": "storage",
          "resourceName": "available",
          "attributes": [
            "Name",
            "Arn",
            "StreamArn"
          ]
        }
      ]
    },
    "getSheets": {
      "build": true,
      "providerPlugin": "awscloudformation",
      "service": "Lambda",
      "dependsOn": [
        {
          "category": "storage",
          "resourceName": "sheets",
          "attributes": [
            "Name",
            "Arn",
            "StreamArn"
          ]
        }
      ]
    },
    "syncSheets": {
      "build": true,
      "providerPlugin": "awscloudformation",
      "service": "Lambda",
      "dependsOn": [
        {
          "category": "storage",
          "resourceName": "sheets",
          "attributes": [
            "Name",
            "Arn"
          ],
          "attributeEnvMap": {
            "Name": "TABLE_NAME",
            "Arn": "TABLE_ARN"
          }
        }
      ]
    },
    "getApplicationInfo": {
      "build": true,
      "providerPlugin": "awscloudformation",
      "service": "Lambda",
      "dependsOn": [
        {
          "category": "storage",
          "resourceName": "available",
          "attributes": [
            "Name",
            "Arn",
            "StreamArn"
          ]
        },
        {
          "category": "storage",
          "resourceName": "sheets",
          "attributes": [
            "Name",
            "Arn",
            "StreamArn"
          ]
        }
      ]
    }
  },
  "auth": {},
  "api": {
    "available": {
      "service": "API Gateway",
      "providerPlugin": "awscloudformation",
      "dependsOn": [
        {
          "category": "function",
          "resourceName": "getAvailable",
          "attributes": [
            "Name",
            "Arn"
          ]
        },
        {
          "category": "function",
          "resourceName": "syncAvailable",
          "attributes": [
            "Name",
            "Arn"
          ]
        },
        {
          "category": "function",
          "resourceName": "getSheets",
          "attributes": [
            "Name",
            "Arn"
          ]
        },
        {
          "category": "function",
          "resourceName": "syncSheets",
          "attributes": [
            "Name",
            "Arn"
          ]
        },
        {
          "category": "function",
          "resourceName": "getApplicationInfo",
          "attributes": [
            "Name",
            "Arn"
          ]
        }
      ]
    }
  }
}